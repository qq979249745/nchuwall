
.a{
  width: 100%;
  display: flex;
  text-align: center;
  flex-direction: column;
}
.a image{
  margin: 0 auto;
  text-align: center;
  width: 300rpx;
  height: 300rpx;
}
.a text{
  margin: 0 auto;
  width: 85%;
}
.a button{
  background-color: #e07883;
  margin-top: 20rpx;
  color: white;
  height: 50px;
  width: 60%;
}

.b{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.b-image{
  width: 128rpx;
  height: 128rpx;
  margin: 10rpx;
  border-radius: 50%;
}
.c{
  height: 100rpx;
  width: 100%;
}
.c text{
  line-height: 100rpx;
  float: left;
  font-size: 35rpx;
  margin-left: 20rpx;
}
.d{
  margin-top: 20rpx;
  margin-left: 20rpx;
  width: 60rpx;
  height: 60rpx;
  float: left;
}
.e{
  margin-top: 20rpx;
  margin-right: 20rpx;
  width: 60rpx;
  height: 60rpx;
  float: right;
}
.f{
  width: 100%;
  height: 1rpx;
  margin-top: 5rpx;
  background-color: gainsboro;
}
.g{
  width: 100%;
  height: 20rpx;
  background-color: #f3f5f9;
}